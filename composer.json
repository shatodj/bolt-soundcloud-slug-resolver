{
    "name": "shatodj/bolt-soundcloud-slug-resolver",
    "description": "Bolt CMS Soundcloud Slug Resolver Extension.",
    "type": "bolt-extension",
    "version": "1.0.0",
    "keywords": [
        "bolt", "boltcms", "extension", "Soundcloud", "API", "twig"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "njasm/soundcloud": "dev-master@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub Šatník",
            "email": "kubo1988@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\SHatoDJ\\Soundcloud\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\SHatoDJ\\Soundcloud\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\SHatoDJ\\Soundcloud\\SoundcloudSlugResolverExtension"
    }
}
